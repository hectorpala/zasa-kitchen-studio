{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/*.zip"
    ],
    "rewrites": [
      {
      },
      {
      }
    ],
    "headers": [
      {
        "source": "**/*.@(css|js|jpg|jpeg|png|svg|webp)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000"
          }
        ]
      }
    ]
  }
}